{
  "manifest_version": 2,

  "name": "HeSaidSheSaid Plugin",
  "description": "This extension will analyze a page",
  "version": "1.0",

  
  "browser_action": {
   "default_icon": "u-dont-fuckin-control-me-dog-meme.jpg",
   "default_popup": "popup.html"
  },
  
  	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": ["jquery-2.2.3.min.js","content.js"],
			"run_at": "document_end"
		}
	],
  
  "permissions": [
   "activeTab"
   ]
}
